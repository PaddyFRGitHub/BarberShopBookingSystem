<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookingsystem">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookingSystem">
        <activity
            android:name=".MainActivityNewUser"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityHome"
            android:theme="@style/Theme.BookingSystem" />
        <activity
            android:name=".MainActivityBook"
            android:theme="@style/Theme.BookingSystem"/>
        <activity
            android:name=".MainActivityManage"
            android:theme="@style/Theme.BookingSystem"/>
        <activity
            android:name=".MainActivityAnnouncements"
            android:theme="@style/Theme.BookingSystem"/>
        <activity
            android:name=".BookingDetailsActivity"
            android:theme="@style/Theme.BookingSystem"/>

    </application>


</manifest>